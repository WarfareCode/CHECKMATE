    <Receivers>
        <CookieCutterReceiver>
            <name>ccr</name>
            <detection-range>50.</detection-range>
            <scan-period>10.</scan-period>
        </CookieCutterReceiver>
        <SensitivityReceiver>
            <name>sr</name>
            <sensitivity>-70.</sensitivity>
            <freq-low>2000.</freq-low>
            <freq-high>18000.</freq-high>
            <scan-period>10.</scan-period>
        </SensitivityReceiver>
        <TableLookupReceiver>
            <name>tlr</name>
            <detection-range>
                <classification>0</classification>
                <range>100.</range>
            </detection-range>
            <detection-range>
                <classification>1</classification>
                <range>300.</range>
            </detection-range>
            <detection-range>
                <classification>5</classification>
                <range>300.</range>
            </detection-range>
            <scan-period>10.</scan-period>
        </TableLookupReceiver>
        <ThreeLevelTableLookupReceiver>
            <name>tlr2</name>
            <simdis-display value="true"/>
            <detection-range>
                <classification>0</classification>
                <mainlobe-range>100.</mainlobe-range>
                <sidelobe-range>60.</sidelobe-range>
                <ave-sidelobe-range>30.</ave-sidelobe-range>
                <simdis-color>yellow</simdis-color>
                <simdis-range>100.</simdis-range>
            </detection-range>
            <detection-range>
                <classification>1</classification>
                <mainlobe-range>300.</mainlobe-range>
                <sidelobe-range>180.</sidelobe-range>
                <ave-sidelobe-range>90.</ave-sidelobe-range>
                <simdis-color>blue</simdis-color>
            </detection-range>
            <detection-range>
                <classification>5</classification>
                <mainlobe-range>300.</mainlobe-range>
                <sidelobe-range>180.</sidelobe-range>
                <ave-sidelobe-range>90.</ave-sidelobe-range>
            </detection-range>
            <scan-period>10.</scan-period>
        </ThreeLevelTableLookupReceiver>
    </Receivers>
    <Jammers>
        <BasicJammer>
            <name>bj</name>
            <reference-range>100.</reference-range>
            <pa-effectiveness>.2</pa-effectiveness>
            <ra-effectiveness>.8</ra-effectiveness>
        </BasicJammer>
        <AssignmentsJammer>
            <name>aj</name>
            <total-resources>8</total-resources>
            <jamming-assignment type="PREEMPTIVE">
                <resources-required>1</resources-required>
                <Coverage>
                    <classification>0</classification>
                    <reference-range>100.</reference-range>
                    <effectiveness>0.4</effectiveness>
                </Coverage>
                <Coverage>
                    <classification>1</classification>
                    <reference-range>100.</reference-range>
                    <effectiveness>0.4</effectiveness>
                </Coverage>
            </jamming-assignment>
            <jamming-assignment type="REACTIVE">
                <resources-required>1</resources-required>
                <Coverage>
                    <classification>0</classification>
                    <reference-range>100.</reference-range>
                    <effectiveness>0.8</effectiveness>
                </Coverage>
            </jamming-assignment>          
        </AssignmentsJammer>
    </Jammers>
